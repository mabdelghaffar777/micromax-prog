            function startCarousel(){
                $("#phone-features .owl-carousel").owlCarousel({
                    loop: true,
                    margin: 15,
                    nav: true,
                    paginationSpeed : 400,
                    itemsDesktop : false,
                    itemsDesktopSmall : false,
                    itemsTablet: false,
                    itemsMobile : false,
