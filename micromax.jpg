                    $(ele).find("i").toggleClass("fa-play fa-pause");
                }
                
                if(vid){
                    vid.onended = function() {
                        toggleVnewClass("#homeVideo-playBtn");
                        vid.load();
                    };
                }
                
                $("#homeVideo-playBtn").click(function(){
                    if(!$(".btn-play-video").hasClass('pause')){
                        vid.play();
                        } else {
                        vid.pause();
                    }
                    toggleVnewClass("#homeVideo-playBtn");
                });		
				
				/*************************************** */
                
                var vidG = document.getElementById("videoG");
                
                function toggleVClass(ele){
					$(".btn-play-video2").toggleClass("pause");
